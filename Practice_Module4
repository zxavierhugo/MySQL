/*------------Creation of customer -----------*/

CREATE TABLE module4.OrdLine (
OrdNo CHAR(8),
ProdNo CHAR(8),
Qty INTEGER DEFAULT 1,
CONSTRAINT PKOrdLine PRIMARY KEY (OrdNo, ProdNo),
CONSTRAINT FKOrdNo_OrdLine FOREIGN KEY (OrdNo) REFERENCES OrderTbl(OrdNo)
ON DELETE CASCADE,
CONSTRAINT FKProdNo_OrdLine FOREIGN KEY (ProdNo) REFERENCES Product(ProdNo)
);

