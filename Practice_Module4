/*------------Creation of product -----------*/

CREATE TABLE module4.Product (
ProdNo CHAR(8),
ProdName VARCHAR(50) NOT NULL,
ProdMfg VARCHAR(20) NOT NULL,
ProdQOH  DECIMAL(12,2),
ProdPrice DECIMAL(12,2),
ProdNextShipDate DATE,
CONSTRAINT PKProduct PRIMARY KEY (ProdNo)
);

