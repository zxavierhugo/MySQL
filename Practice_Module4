/*------------Creation of employee -----------*/

CREATE TABLE module4.Employee (
EmpNo CHAR(8),
EmpFirstName VARCHAR(20) NOT NULL,
EmpLastName VARCHAR(20) NOT NULL,
EmpPhone CHAR(15),
EmpEmail VARCHAR(50) NOT NULL,
SupEmpNo CHAR(8),
EmpCommRate DECIMAL(3,3),
CONSTRAINT PKEmployee PRIMARY KEY (EmpNo),
CONSTRAINT FKSupEmpNo_Employee FOREIGN KEY(SupEmpNo) REFERENCES Employee(EmpNo),
CONSTRAINT UnqiueEmpEmail UNIQUE (EmpEmail)
);

